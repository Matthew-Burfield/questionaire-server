{
  "version": 2,
  "builds": [{ "src": "src/index.js", "use": "@now/node-server" }],
  "alias": ["burfield-questionaire-server"],
  "engines": {
    "node": "11.6.0"
  },
  "now": {
    "engines": {
      "node": "11.6.0"
    }
  },
  "scripts": {
    "start": "node ./src/index.js"
  },
  "routes": [
    {
      "src": "/.*",
      "dest": "/index.ts"
    }
  ]
}
